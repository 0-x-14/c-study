#include <stdio.h>
#include <string.h>

int main() {
	char str[100];
	scanf_s("%d", str, sizeof(str));
	int n = strlen(str);
	for (int i = 0; i < strlen(str); i++) {
		if ((str[i] == 'l' || str[i] == 'n') && str[i + 1] == 'j')
			n--;
		if (str[i] == 'd' && str[i + 1] == 'z' && str[i + 2] == '=')
			n--;
		if (str[i] == '=' || str[i] == '-')
			n--;
	}
	printf("%d", n);
	return 0;
}
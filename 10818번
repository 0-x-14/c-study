#include <stdio.h>

int main() {
	int num;
	int arr[1000001];

	scanf_s("%d", &num);
	for (int i = 0; i < num; i++) {
		scanf_s("%d", &arr[i]);
	}

	int min = arr[0];
	int max = arr[0];

	for (int i = 0; i < num; i++) {
		if (min > arr[i]) {
			min = arr[i];
		}
		else if (max < arr[i]) {
			max = arr[i];
		}
	}

	printf("%d %d", min, max);
}